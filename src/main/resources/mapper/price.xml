<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.springboot.mapper.ProductMapper">
    <insert id="insert">
        insert into product_tb
        values (default, #{productName}, #{size}, #{price})
    </insert>
    <select id="findProduct">
        select
            *
        from
            product_tb
    </select>
</mapper>